{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": ["eslint:recommended"],
    "plugins": [
        "standard"
    ],
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "no-console": "off",
        "no-empty": "off"
    }
}